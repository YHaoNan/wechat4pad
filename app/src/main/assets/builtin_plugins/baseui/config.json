{
    "name": "BaseUI",
    "desc": "Provide a basic UI and some UI API",
    "author": "LILPIG",
    "icon": "data:image/png;base64,",
    "showup_in_plugin_list": true,
    "settings": [{
        "id": "wx4pad.baseui.sendmsgbyenter",
        "name": "回车发送",
        "desc": "启用后界面上的发送按钮变成换行键",
        "type": "bool",
        "default": true
    }, {
        "id": "wx4pad.baseui.nightmode",
        "name": "夜间模式",
        "desc": "界面将变成适合夜间使用的暗色系",
        "type": "bool",
        "default": false
    }],
    "inject": ["js/base.js", "js/top_drawer.js", "js/plugin_list.js", "js/input_method.js", "js/enter_send.js", "css/base.css", "css/drawer.css", "css/plugin_list.css"]
}